/******************************************/
QPushButton {
    font:normal 14px ;
    color:#333333; /* veryLight */
    background:transparent;
    padding:2px;
    border:none;
    border-radius:2px;
}
QPushButton:disabled {
	color:#9E9E9E;
	border:none;
	background: transparent;
}
QPushButton::menu-indicator {
    background:transparent;
}

QPushButton[ButtonStyle="normal"]{
	font:normal 14px ;
	color: #668196;
	border-radius: 2px;
	background: #FFFFFF;
	border: 1px solid #DCDFE6;
}
QPushButton[ButtonStyle="normal"]:hover{
	background: #E9ECEE;
}
QPushButton[ButtonStyle="normal"]:pressed{
	background: #D9DEE0;
}

QPushButton[ButtonStyle="blue"]{
	font: normal 14px ;
	color:#FAFAFA;
	background:rgba(10,141,255,1);
	border-radius:2px ;
}
QPushButton[ButtonStyle="blue"]:hover{
	background:rgba(10,141,255,0.8);
}
QPushButton[ButtonStyle="blue"]:pressed{
	background:rgba(10,141,255,0.9);
}

QPushButton[ButtonStyle="red"]{
	font: normal 14px ;
	color:#FAFAFA;
	border-radius:2px;
	background:#F44336;
}
QPushButton[ButtonStyle="red"]:hover{
	background: #FF5252;
}
QPushButton[ButtonStyle="red"]:pressed{
	background: #E53935;
}

/******************************************/
QSpinBox{
	font:normal 14px;
