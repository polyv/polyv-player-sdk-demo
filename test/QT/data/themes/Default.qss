	background-color: white; /* veryDark */
}

/******************************************/
QComboBox {
    font:normal 14px ;
    color:#668196;
    border-radius: 2px;
    padding: 2px;
    padding-left: 4px;
    border: 1px solid #CFD8DC;
    combobox-popup: 0;
	background-color: white; /* veryDark */
}
QComboBox:hover{
    border: 1px solid #BCDFFB;
}
QComboBox:focus{
    border: 1px solid #BCDFFB;
}
QComboBox::drop-down {
    width:24px;
    height:24px;
    border:none;
    subcontrol-origin:padding;
    subcontrol-position: right center;
    padding-right:6px;
}
QComboBox:on {
    background-color: white;
}
QComboBox:disabled{
    color:#757575;
}
QComboBox::drop-down:editable {
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}
QComboBox::down-arrow:editable {
    qproperty-alignment: AlignTop;
    width: 100%;
}
QComboBox::down-arrow:!on {
    width:40px;
    height:40px;
    image: url(:res/images/control/icon_down_normal_white.svg);
}
QComboBox::down-arrow:!on:hover {
    image: url(:res/images/control/icon_down_active.svg);
}
QComboBox::down-arrow:!on:pressed {
    image: url(:res/images/control/icon_down_active.svg);
}
QComboBox::down-arrow:!on:disabled {
    image: url(:res/images/control/icon_down_normal_white.svg);
}
QComboBox::down-arrow:on {
    width:40px;
    height:40px;
    image: url(:res/images/control/icon_up_normal_white.svg);
}
QComboBox::down-arrow:on:hover {
    image: url(:res/images/control/icon_up_active.svg);
}
QComboBox::down-arrow:on:pressed {
    image: url(:res/images/control/icon_up_active.svg);
}
QComboBox::down-arrow:on:disabled {
    image: url(:res/images/control/icon_up_normal_white.svg);
}

/******************************************/
QLineEdit, QTextEdit {
    background-color: white; /* veryDark */
    border: 1px solid #CFD8DC;
	font:normal 14px;
	color: #9E9E9E;
    padding-left: 4px;
}
QLineEdit:hover,QTextEdit:hover {
	border-radius: 2px;
	border: 1px solid #2196F3;
}

/******************************************/
QListWidget {
	font:normal 12px ;
    show-decoration-selected: 1; /* make the selection span the entire width of the view */ 
}
QListWidget::item{
	background:white;
	color:#546e7a;
}
QListWidget::item:selected {
	background:#e3f2fd;
}

/******************************************/
::corner {
    background-color: rgb(255,255,255); /* dark */
    border: none;
}

QScrollBar:vertical {
    background-color:#f1f1f1;
	width: 14px;
}

QScrollBar::handle:vertical {
    background-color:#c1c1c1;
    min-height: 20px;
    margin: 2px;
    border-radius:5px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    border: none;
    background: none;
}

QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical, QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    border: none;
    background: none;
    color: none;
}

QScrollBar:horizontal {
    background-color:#f1f1f1;
    height: 14px;
}

QScrollBar::handle:horizontal {
    background-color:#c1c1c1;
    min-width: 20px;
    margin: 2px;
    border-radius: 5px;
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    border: none;
    background: none;
}

QScrollBar::left-arrow:horizontal, QScrollBar::right-arrow:horizontal, QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    border: none;
    background: none;
    color: none;
}

/******************************************/
QSlider::handle:hover {
    background-color:#e3f2fd; /* veryLight */
}
QSlider::handle:disabled {
    background-color: white; /* light */
}
QSlider::sub-page {
    background-color: rgb(42,130,218); /* blue */
    border-radius: 2px;
}
/* horizontal */
QSlider::groove:horizontal {
  background-color: rgba(255, 255, 255, 0.3);
  height: 4px;
  border: none;
  /*border-radius: 2px;*/
}
QSlider::sub-page:horizontal{
    background-color:#1D60FF; 
    /*border-radius: 2px;*/
}
QSlider::handle:horizontal {
    background:transparent;
	border-image:url(:res/images/control/slider_handle.png);
    height: 14px;
    width: 14px;
    margin-top: -5px;
	margin-bottom: -5px;  /*handle is placed by default on the contents rect of the groove. Expand outside the groove */
}
QSlider::handle:horizontal:hover{
    background-color:transparent;
}
QSlider::handle:horizontal:pressed {
    background-color:transparent;
}
/* vertical */
QSlider::groove:vertical {
    background-color: rgba(255, 255, 255, 0.3);
    width: 4px;
    border: none;
    border-radius: 2px;
}
QSlider::add-page:vertical{
    background-color:#1D60FF; 
}
QSlider::sub-page:vertical{
    background-color:rgba(255, 255, 255, 0.3); 
}
QSlider::handle:vertical {
    background:transparent;
    border-image:url(:res/images/control/slider_handle.png);
    width: 14px;
    height: 14px;
    margin-left: -5px;
	margin-right: -5px;  /*handle is placed by default on the contents rect of the groove. Expand outside the groove */
}
QSlider::handle:vertical:hover{
    background-color:transparent;
}
QSlider::handle:vertical:pressed {
    background-color:transparent;
}

